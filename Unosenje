package projekat;

import java.util.Scanner;

public class unosenje {

	public static void main(String[] args) {
		Scanner unos=new Scanner(System.in);
		System.out.println("Unesite prvi broj: ");
		double prviBroj=unos.nextInt();
		System.out.println("Unesite drugi broj: ");
		double drugiBroj=unos.nextInt();
		double suma=prviBroj+drugiBroj;
		double razlika=prviBroj-drugiBroj;
		double proizvod=prviBroj*drugiBroj;
		double kolicnik=prviBroj/drugiBroj;
		double prviKorijen=Math.sqrt(prviBroj);
		double drugiKorijen=Math.sqrt(drugiBroj);
		System.out.println("Suma dva broja je: " + suma);
		System.out.println("Razlika dva broja je: " + razlika);
		System.out.println("Proizvod dva broja je: " + proizvod);
		System.out.println("Kolicnik dva broja je: " + kolicnik);
		System.out.println("Korijen prvog broja je: " + prviKorijen);
		System.out.println("Korijen drugog broja je: " + drugiKorijen);
	}

}
